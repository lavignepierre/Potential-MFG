@article{gabay,
title = "A dual algorithm for the solution of nonlinear variational problems via finite element approximation",
journal = "Computers & Mathematics with Applications",
volume = "2",
number = "1",
pages = "17 - 40",
year = "1976",
issn = "0898-1221",
doi = "https://doi.org/10.1016/0898-1221(76)90003-1",
url = "http://www.sciencedirect.com/science/article/pii/0898122176900031",
author = "Daniel Gabay and Bertrand Mercier",
abstract = "For variational problems of the form Infv∈V{f(Av)+g(v)}, we propose a dual method which decouples the difficulties relative to the functionals f and g from the possible ill-conditioning effects of the linear operator A. The approach is based on the use of an Augmented {L}agrangian functional and leads to an efficient and simply implementable algorithm. We study also the finite element approximation of such problems, compatible with the use of our algorithm. The method is finally applied to solve several problems of continuum mechanics."
}

